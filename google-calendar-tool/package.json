{
  "name": "google-calendar-tool",
  "version": "1.0.0",
  "description": "Google Calendar integration tool for ElevenLabs conversational AI",
  "main": "src/components/GoogleCalendarTool.js",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .js,.jsx",
    "lint:fix": "eslint src --ext .js,.jsx --fix"
  },
  "keywords": [
    "google-calendar",
    "elevenlabs",
    "conversational-ai",
    "calendar",
    "oauth",
    "react"
  ],
  "author": "ab2005@gmail.com",
  "license": "MIT",
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "googleapis": "^126.0.1",
    "google-auth-library": "^9.4.0",
    "axios": "^1.6.2",
    "date-fns": "^2.30.0",
    "uuid": "^9.0.1",
    "prop-types": "^15.8.1"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.2.0",
    "vite": "^5.0.8",
    "@testing-library/react": "^14.1.2",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/user-event": "^14.5.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "@babel/preset-env": "^7.23.6",
    "@babel/preset-react": "^7.23.3",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-jest": "^27.6.0"
  },
  "jest": {
    "testEnvironment": "jsdom",
    "setupFilesAfterEnv": ["<rootDir>/tests/setupTests.js"],
    "moduleNameMapping": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/**/*.test.{js,jsx}",
      "!src/**/*.spec.{js,jsx}"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "babel": {
    "presets": [
      ["@babel/preset-env", { "targets": { "node": "current" } }],
      ["@babel/preset-react", { "runtime": "automatic" }]
    ]
  }
}